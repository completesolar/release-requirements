{
    "name": "completesolar/release-requirements",
    "description": "Add auto launch of requirements for laravel project.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "CompleteSolar\\ReleaseRequirement\\": "src/",
            "CompleteSolar\\ReleaseRequirement\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0"
    },
    "config": {
        "php": "^8.1",
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CompleteSolar\\ReleaseRequirement\\ReleaseRequirementProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0"
    }
}
